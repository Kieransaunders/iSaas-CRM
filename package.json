{
  "name": "isaasit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/workos/template-convex-tanstack-start-authkit/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workos/template-convex-tanstack-start-authkit.git"
  },
  "bugs": {
    "url": "https://github.com/workos/template-convex-tanstack-start-authkit/issues"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend dev:docs",
    "dev:frontend": "vite dev",
    "dev:backend": "convex dev",
    "dev:all": "npm run dev",
    "dev:docs": "cd docs && npm run dev",
    "predev": "convex dev --until-success",
    "build": "vite build",
    "build:docs": "cd docs && npm run build",
    "build:combined": "npm run build && npm run build:docs-combined",
    "build:docs-combined": "cd docs && npm install && npm run build && mkdir -p ../dist/client/docs && cp -r dist/* ../dist/client/docs/",
    "start": "vite start",
    "preview:docs": "cd docs && npm run preview",
    "preview:combined": "node scripts/preview-combined.js",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test:e2e": "playwright test",
    "postinstall": "node scripts/post-install.js",
    "setup": "node scripts/setup.js",
    "setup:check": "node --experimental-strip-types scripts/setup-check.ts",
    "setup:gsd": "node scripts/setup-gsd.js",
    "gsd:help": "echo 'Run: npm run setup:gsd' && echo 'Or manually: npx get-shit-done-cc@latest --claude --local'"
  },
  "dependencies": {
    "@convex-dev/polar": "^0.7.3",
    "@convex-dev/react-query": "^0.1.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.158.0",
    "@tanstack/react-router-ssr-query": "^1.158.0",
    "@tanstack/react-start": "^1.158.0",
    "@workos-inc/node": "^8.1.0",
    "@workos/authkit-tanstack-react-start": "0.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@netlify/vite-plugin-tanstack-start": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/react-router-devtools": "^1.158.0",
    "@types/node": "^24.10.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@playwright/test": "^1.49.1",
    "dotenv": "^17.2.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5"
  }
}
